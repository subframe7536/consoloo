{
  "name": "consoloo",
  "type": "module",
  "version": "1.0.1",
  "description": "colorful logger for nodejs and browser",
  "author": "subframe7536",
  "license": "MIT",
  "repository": "https://github.com/subframe7536/consoloo",
  "keywords": [
    "console",
    "logger"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./browser": {
      "import": "./dist/browser.js",
      "require": "./dist/browser.cjs"
    },
    "./core": {
      "import": "./dist/core.js",
      "require": "./dist/core.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "files": [
    "browser.*",
    "core.*",
    "dist"
  ],
  "scripts": {
    "dev": "vite serve dev",
    "build": "tsup",
    "test": "bun tests/index.test.ts",
    "test:clean": "bun tests/index.test.ts clean",
    "lint": "eslint . --fix",
    "release": "pnpm test:clean && pnpm build && bumpp --all && npm publish"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@subframe7536/type-utils": "^0.1.6",
    "colorette": "^2.0.20",
    "normal-error": "^0.2.0"
  },
  "devDependencies": {
    "@subframe7536/eslint-config": "^0.8.0",
    "@types/node": "^20.16.2",
    "bumpp": "^9.5.2",
    "eslint": "^9.9.1",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "vite": "^5.4.2"
  }
}
