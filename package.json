{
  "name": "consoloo",
  "type": "module",
  "version": "0.8.1",
  "description": "colorful logger for nodejs and browser",
  "author": "subframe7536",
  "license": "MIT",
  "keywords": [
    "console",
    "logger"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./browser": {
      "import": "./dist/browser.js",
      "require": "./dist/browser.cjs"
    },
    "./core": {
      "import": "./dist/core.js",
      "require": "./dist/core.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "files": [
    "browser.*",
    "core.*",
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "esno tests/index.test.ts",
    "test:clean": "esno tests/index.test.ts clean",
    "lint": "eslint . --fix",
    "release": "pnpm test:clean && pnpm build && bumpp && npm publish"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@subframe7536/type-utils": "^0.1.3",
    "normal-error": "^0.1.4",
    "picocolors": "^1.0.0"
  },
  "devDependencies": {
    "@subframe7536/eslint-config": "^0.3.7",
    "@types/node": "^20.8.10",
    "bumpp": "^9.2.0",
    "eslint": "^8.52.0",
    "esno": "0.17.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  }
}
