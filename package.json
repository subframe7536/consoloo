{
  "name": "consoloo",
  "type": "module",
  "version": "2.0.0",
  "description": "colorful logger for nodejs and browser",
  "author": "subframe7536",
  "license": "MIT",
  "repository": "https://github.com/subframe7536/consoloo",
  "keywords": [
    "console",
    "logger"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./browser": "./dist/browser.js",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "bun ./dev/index.html",
    "build": "tsdown",
    "test": "bun test",
    "format": "eslint . --fix",
    "release": "bun run test && bun run build && bumpp --all && npm publish"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "ansis": "^4.1.0",
    "normal-error": "^0.2.0"
  },
  "devDependencies": {
    "@subframe7536/eslint-config": "^1.3.2",
    "@subframe7536/type-utils": "^0.2.0",
    "@types/node": "^22.17.1",
    "bumpp": "^10.2.2",
    "bun-types": "^1.2.20",
    "eslint": "^9.33.0",
    "tsdown": "^0.14.0",
    "typescript": "^5.9.2"
  }
}
